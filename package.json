{
  "name": "car-store-server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node dist/server.js",
    "start:dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "create-admin": "ts-node-dev --respawn --transpile-only src/scripts/createAdmin.ts",
    "start:preprod": "tsc-watch --onSuccess \"node dist/server.js\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint:strict": "eslint . --max-warnings=0",
    "check": "yarn run format:check && yarn run lint:strict",
    "build": "tsc"
  },
  "keywords": [],
  "author": "hanif",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.10.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/node": "^22.13.4",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.3"
  }
}
